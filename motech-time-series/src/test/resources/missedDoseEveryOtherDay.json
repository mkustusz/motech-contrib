{
    "name":"missedDoseEveryOtherDay",
    "type":{
        "name":"onRecord"
    },
    "transformations":{
        "head":{
            "function":{
                "name":"regex",
                "configuration":{
                    "regex":"(0.01.0).+"
                }
            },
            "dataSource":{
                "type":"points",
                "query":{
                    "number":"5"
                }
            }
        },
        "line":[
            {
                "function":{
                    "name":"threshold",
                    "configuration":{
                        "threshold":"0",
                        "lessThan":"false"
                    }
                }
            }
        ]
    }
}
